<div .container>
    <h1>Snapshots
    $forall stackages <- groups
        $forall (_, _, uploaded, _, _) <- take 1 stackages
            <h3>
                #{uploaded}
        <ul .snapshots>
            $forall (ident, title, uploaded, display, handle) <- stackages
                <li>
                    <strong>
                        <a href=@{StackageHomeR ident}>
                            #{title}
                    <p>
                        #{display} (#{handle})
